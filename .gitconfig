[user]
	email = satojkovic@gmail.com
	name = satojkovic
[github]
	user = satojkovic
[color]
	ui = true
[alias]
	br = branch
	c = commit
	cm = commit -m
	co = checkout
	df = diff
	l = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s%C(white)- %an, %ar%Creset'
	lg = log --graph --pretty=oneline --decorate --date=short --abbrev-commit --branches
	ll = log --stat --abbrev-commit
	pr = pull --rebase
	st = status -sb
	cgl = config --global --list
	cll = config --local --list
	spull = svn rebase
	spush = svn dcommit
	bc = bundle create
	lsr = ls-remote
	rv = remote -v
	ls = ls-files
	ra = remote add
	ss = stash save
	sl = stash list
	sd = stash drop
	sp = stash pop
	sa = stash apply
[core]
	excludesfile = /Users/satojkovic/.gitignore
	pager = less
[merge]
	ff = false
[push]
	default = simple
